{
  "name": "laravel-docker-base",
  "version": "0.0.1",
  "description": "Configures a new laravel project with docker support for local development",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node .",
    "docker:on": "docker-compose up -d",
    "docker:off": "docker-compose down",
    "docker:remote": "docker exec -it <PROJECT_NAME> sh",
    "docker:rebuild": "docker-compose build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmyaldape/laravel-docker-base.git"
  },
  "keywords": [
    "laravel",
    "docker",
    "base"
  ],
  "author": "Jimmy <jimmy@jimmyaldape.com> (https://jimmyaldape.dev)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jimmyaldape/laravel-docker-base/issues"
  },
  "homepage": "https://github.com/jimmyaldape/laravel-docker-base#readme",
  "dependencies": {
    "boxen": "^4.2.0",
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "replace-in-file": "^6.1.0"
  },
  "engines": {
    "node": ">=12.10.0",
    "npm": ">=6.0.0"
  },
  "private": true
}
